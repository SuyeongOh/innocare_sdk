            app:layout_constraintBottom_toTopOf="@+id/stress_value"
            app:layout_constraintRight_toLeftOf="@+id/late_stress_ews_unit"
            app:ticker_animationDuration="500"
            app:ticker_defaultCharacterList="number"
            tools:text="2" />

