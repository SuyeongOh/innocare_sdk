            tools:text="TBD" />

        <TextView
            android:id="@+id/late_BP_value_unit2"
            style="@style/InfoContainerUnitTextView"
            android:text="@string/blood_pressure_unit"
            app:layout_constraintBottom_toBottomOf="@+id/lowest_blood_pressure_value"
            app:layout_constraintEnd_toEndOf="@+id/late_BP_value_unit"
            app:layout_constraintStart_toStartOf="@+id/late_BP_value_unit"
            app:layout_constraintTop_toTopOf="@+id/lowest_blood_pressure_value" />

        <ProgressBar
            android:id="@+id/blood_pressure_progress"
            style="?android:attr/progressBarStyle"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:indeterminateDrawable="@drawable/circle_progress"
            android:indeterminateDuration="1000"
            android:visibility="gone"
            app:layout_constraintHeight_percent="0.4"
            app:layout_constraintDimensionRatio="1"
            app:layout_constraintStart_toEndOf="@+id/blood_pressur_img"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/blood_pressur_img"
            app:layout_constraintBottom_toBottomOf="@+id/blood_pressur_img"
            tools:visibility="visible"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ecg_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintTop_toBottomOf="@id/oxygen_saturation_container"
        app:layout_constraintBottom_toTopOf="@id/ppg_container"
        app:layout_constraintStart_toStartOf="parent">

        <com.androidplot.xy.XYPlot
            android:id="@+id/ecg_plot"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:graphBackgroundColor="@color/colorAccent"
            app:graphMarginBottom="12dp"
            app:graphMarginTop="12dp"
            app:gridBackgroundColor="@color/colorAccent"
            app:rangeTitleTextColor="@android:color/black"
            app:rangeTitleTextSize="8sp"
            app:lineLabels="left"
            app:rangeTitle="mV"
            app:legendTextSize="8sp"
            app:renderMode="use_background_thread"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="parent"
            app:layout_constraintWidth_percent="0.7" />

        <TextView
            android:id="@+id/ecg_hr"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:layout_constraintStart_toEndOf="@id/ecg_plot"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="@color/white70"
            android:gravity="center"
            android:textSize="14sp"
            tools:text="HR : 90"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ppg_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <com.androidplot.xy.XYPlot
            android:id="@+id/ppg_plot"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:graphBackgroundColor="@color/colorAccent"
            app:graphMarginBottom="12dp"
            app:graphMarginTop="12dp"
            app:gridBackgroundColor="@color/colorAccent"
            app:rangeTitleTextColor="@android:color/black"
            app:rangeTitleTextSize="8sp"
            app:lineLabels="left"
            app:rangeTitle="mV"
            app:legendTextSize="8sp"
            app:renderMode="use_background_thread"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="parent"
            app:layout_constraintWidth_percent="0.7" />

        <TextView
            android:id="@+id/ppg_hr"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            app:layout_constraintStart_toEndOf="@id/ppg_plot"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="@color/white70"
            android:gravity="center"
            android:textSize="14sp"
            tools:text="HR : 90"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>