        <TextView
            android:id="@+id/late_HRV_SDNN_value_unit"
            style="@style/InfoContainerUnitTextView"
            android:text="@string/hrv_sdnn_unit"
            app:layout_constraintBottom_toBottomOf="@+id/hrv_sdnn_value"
